   "source": [
    "def is_palindrome(text):\n",
    "    cleaned = \"\"\n",
    "    for char in text.lower():\n",
    "        if char.isalnum():    \n",
    "            cleaned += char\n",
    "    return cleaned == cleaned[::-1]\n",
    "\n",
    "is_palindrome(\"rcecar\")"
   ]
  }
