    "def reverse_string(text):\n",
    "    reversed_text = \"\"\n",
    "    for char in text:\n",
    "        reversed_text = char + reversed_text\n",
    "    return reversed_text\n",
    "reverse_string(\"Big ol honkin hamburger\")"
   ]
