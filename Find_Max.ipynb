    "def find_max(values):\n",
    "    current_max = values[0]\n",
    "\n",
    "    for num in values:\n",
    "        if num > current_max:\n",
    "            current_max = num\n",
    "\n",
    "    return current_max\n",
    "\n",
    "find_max([4, 6, 234, 25, 22222])"
   ]
